{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "../../dist/out-tsc/lib",
    "declaration": true,
    "declarationMap": true,
    "inlineSources": true,
    "target": "ES2022",
    "lib": ["ES2022", "DOM"],
    "module": "ES2022",
    "types": []
  },
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts",
    "jest.config.ts",
    "src/**/*.test.ts"
  ],
  "include": ["src/**/*.ts"]
}
